<br></br><br></br>
<class="Wishlist">
  <container class="d-flex bg-white justify-content-center flex-column">
    <div class="d-flex bg-white justify-content-center my-2">
      <img src="images/Lego_Heart.png" alt="Lego heart" width="80" height="80" />
    </div>
    <h1 class="text-center fw-bold">WISHLIST</h1>
  </container>
  <section class="wishlist-table">
    <article class="wishlist-table-hdr">
      {{!-- <p>Product Details</p> --}}
      <div>
        <input placeholder="wishlist-product" />
        <button class="wishlist-add_new">+ Add New</button>
      </div>
    </article>
    <article class="wishlist-table_section">
      <table class="table table-striped">
        <thead>
          <tr>
            {{!-- <th scope="col">List #</th> --}}
            <th scope="col">Item #</th>
            <th scope="col">Image</th>
            <th scope="col">Set Name</th>
            <th scope="col">Price</th>
            <th scope="col">Availability</th>
          </tr>
        </thead>
        <tbody>
          {{#each wishlist}}
          <tr>
            {{!-- <th scope="row">{{collection.length}}</th> --}}
            <td>{{set_num}}</td>
            <td><img src={{set_img_url}} /></td>
            <td>{{name}}</td>
            <td>Feature coming soon!</td>
            <td>Feature coming soon!</td>
          </tr>
          {{/each}}

        </tbody>
      </table>
    </article>
    <article>
      <nav aria-label="Page navigation">
        <ul class="pagination justify-content-center mb-5">
          <li class="page-item">
            <a class="page-link">Previous</a>
          </li>
          <li class="page-item"><a class="page-link" href="#">1</a></li>
          <li class="page-item"><a class="page-link" href="#">2</a></li>
          <li class="page-item"><a class="page-link" href="#">3</a></li>
          <li class="page-item">
            <a class="page-link" href="#">Next</a>
          </li>
        </ul>
      </nav>
    </article>
  </section>
  </section>
  </main>
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js"
    integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL"
    crossorigin="anonymous"></script>